{
    "editor.wordWrap":"on",
    "latex-workshop.latex.recipes": [{
        "name": "latexmk",
        "tools": [
          "latexmk"
        ]
    }],
    "latex-workshop.latex.tools": [{
        "name": "latexmk",
        "command": "latexmk",
        "args": [
            "-f",
            "-pdf",
            "-pdflatex=\"pdflatex -synctex=1 -interaction=nonstopmode --shell-escape\"",
            "%DOC%.tex"
        ]
        }
    ],
    "todo-tree.flat": false,
    "todo-tree.regex": "((//|#|<!--|;|/\\*|%|\\\\|^)\\s*($TAGS)|^\\s*- \\[ \\])",
    "todo-tree.tags": [
        "TODO",
        "FIXME",
        "todo"
    ]
}